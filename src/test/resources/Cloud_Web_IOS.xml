<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="EPAM mobile cloud Web testing Suite">
  <parameter name="appType" value="web"></parameter>
  <!--parameter name="deviceName" value="iPhone 8"></parameter-->
  <parameter name="platformName" value="iOS"></parameter>
  <parameter name="browserName" value="Safari"></parameter>
  <parameter name="udid" value="00008030-000D74312233802E"></parameter>
  <parameter name="platformVersion" value="14.6"></parameter>
  <test name="Cloud Web tests">
    <groups>
      <run>
        <include name="web"/>
      </run>
    </groups>
    <packages>
      <package name="scenarios"/>
    </packages>
  </test>
</suite>